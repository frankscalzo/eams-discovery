# EAMS Development Environment Configuration
# This file is copied to .env during dev deployments

# Environment
ENVIRONMENT=dev
STAGE=dev
PROJECT_NAME=eams
SERVICE_NAME=discovery

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=904233104383

# Domain Configuration
DOMAIN_NAME=optimumcloudservices.com
CUSTOM_DOMAIN=dev-discovery.optimumcloudservices.com
CLOUDFRONT_ALIASES=dev-discovery.optimumcloudservices.com,dev-discoveryPWDreset.optimumcloudservices.com

# SSL Certificate (must be in us-east-1 for CloudFront)
CERTIFICATE_ARN=arn:aws:acm:us-east-1:904233104383:certificate/27fab24f-9cc2-4836-914b-1ef8a7b2e438

# Cognito Configuration
COGNITO_DOMAIN_PREFIX=dev-eams
USER_POOL_NAME=dev-eams-users

# DynamoDB Configuration
DYNAMODB_TABLE_PREFIX=dev-eams

# S3 Configuration
S3_BUCKET_PREFIX=dev-eams

# API Gateway Configuration
API_GATEWAY_NAME=dev-eams-api

# Parameter Store Paths
PARAMETER_STORE_PREFIX=/eams/dev
SECRETS_MANAGER_PREFIX=/eams/dev

# Feature Flags
ENABLE_AI_PROCESSING=true
ENABLE_COST_MONITORING=true
ENABLE_BULK_UPLOAD=true
ENABLE_EXCEL_IMPORT=true
ENABLE_DEPENDENCY_MAPPING=true
ENABLE_IMPACT_ANALYSIS=true
ENABLE_REPORTING=true
ENABLE_MULTI_PROJECT=true
ENABLE_CUSTOM_DOMAIN=true
ENABLE_CLOUDFRONT=true
ENABLE_S3_HOSTING=true
ENABLE_DYNAMODB=true
ENABLE_COGNITO=true
ENABLE_API_GATEWAY=true
ENABLE_LAMBDA=true

# External Service Configuration
OPENAI_API_KEY=your-dev-openai-key
ANTHROPIC_API_KEY=your-dev-anthropic-key
AZURE_OPENAI_KEY=your-dev-azure-key
AZURE_OPENAI_ENDPOINT=https://your-dev-resource.openai.azure.com/

# Monitoring and Logging
LOG_LEVEL=debug
ENABLE_CLOUDWATCH_LOGS=true
ENABLE_XRAY_TRACING=true

# Tags
TAG_PROJECT=EAMS
TAG_ENVIRONMENT=dev
TAG_OWNER=OptimumCloudServices
TAG_COST_CENTER=IT-Architecture
TAG_APPLICATION=EnterpriseArchitectureManagement
